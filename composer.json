{
  "name": "zeus-server/zeus-io",
  "description": "A collection of PHP programming language APIs that offer features for intensive I/O operations.",
  "type": "library",
  "license": "LGPL-2.1-only",
  "authors": [
    {
      "name": "Artur Graniszewski",
      "email": "artur.graniszewski@gmail.com"
    }
  ],
  "support": {
    "email": "artur.graniszewski@gmail.com",
    "source": "https://github.com/artur-graniszewski/zeus-io",
    "issues": "https://github.com/artur-graniszewski/zeus-io/issues"
  },
  "homepage": "https://github.com/artur-graniszewski/zeus-io",
  "keywords": [
    "server",
    "socket",
    "stream",
    "file",
    "pipe",
    "buffer",
    "tunnel",
    "selector",
    "selectionKey"
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Zeus\\": "src/Zeus/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZeusIoTest\\": "test/"
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}